@font-face {
  font-family: 'iconfont';  /* project id 217778 */
  src: url('//at.alicdn.com/t/font_no3rmjuc6yw1xlxr.eot');
  src: url('//at.alicdn.com/t/font_no3rmjuc6yw1xlxr.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_no3rmjuc6yw1xlxr.woff') format('woff'),
  url('//at.alicdn.com/t/font_no3rmjuc6yw1xlxr.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_no3rmjuc6yw1xlxr.svg#iconfont') format('svg');
}
.if{
  font-family:"iconfont" !important;
  font-size:16px;font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.if-division:before {
  content: '\e9ca';
}
.if-back:before {
  content: '\e601';
}
.if-minus:before {
  content: '\e760';
}


.inline {
  display: inline-block;
}
.font-12 {
  font-size: 12px;
}
.font-16 {
  font-size: 16px;
}
.font-afafaf {
  color: #afafaf;
}
.flex-wrp{
  height: 100px;
  display:flex;
